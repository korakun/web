<%- include('./admin-layout/header.ejs'); %>        
<main role="main" class="col-md-9 ml-sm-auto col-lg-10 px-4"></main>
                <table class="col-md-9 ml-sm-auto col-lg-10 px-4">
                    <a href="/addPost" class="btn btn-primary">Add Post</a>
                    <link rel="stylesheet" href="css/admin.css">
                    <link rel="stylesheet" href="https://www.w3schools.com/w3css/4/w3.css">
                    <thead>
                        <tr>
                            <th>#</th>
                            <th>file</th>
                            <th>title</th>
                            <th>about</th>
                            <th>date</th>
                            <th>profilePhoto</th>
                        </tr>
                    </thead>
                    <tbody>
                        <% for(var i=0; i< users.length; i++) { %>
                        <tr>
                          <td><%= users[i].post_id %></td>
                          <td><%= users[i].file %></td>
                          <td><%= users[i].title %></td>
                          <td><%= users[i].about %></td>
                          <td><%= users[i].date %></td>
                          <td>
                            <img src="/photos/<%= users[i].picture %>" width="100px" height="100px"/>
                          </td>
                          
                          <td>
                            <a href="/editPost/<%= users[i].post_id %>" class="btn btn-primary">Edit</a>
                            <a href="/deletePost/<%= users[i].post_id %>" class="btn btn-danger">Delete</a>
                          </td>
                        </tr>
                        <% } %>
                  
                      </tbody>
                </table>
            </div>
        </div>
    </div>
</body>
</html>
<%- include('./admin-layout/footer.ejs'); %>  
