        <link rel="stylesheet" href="/css/edit.css" />
	</head>
	
	<body class="col-md-9 ml-sm-auto col-lg-10 px-4" background="https://th.bing.com/th/id/Rc5ea2140710e343706e1d84ded86e5e5?rik=g6IfVRHzi%2ftNOA&pid=ImgRaw"> 
		<div class="login-form">
			<h1>Post Edit</h1>
			<form  action="/editPost/<%= user.post_id %>" method="POST" enctype="multipart/form-data">
				<input type="text" name="name" placeholder="name"  value="<%= user.file %>"   required>
				<input type="title" name="title" placeholder="title" value="<%= user.title %>" required>
                <input type="about" name="about" placeholder="about"  value="<%= user.about %>" required>
				<input type="file" name="picture" placeholder="picture"  value="<%= user.picture %>" required>
                <input type="date" name="date" placeholder="date"  value="<%= user.date %>" required>
				
                
				<input type="submit" class="button" value="Update">  
			</form>
			
		</div>
	</body>
</html>